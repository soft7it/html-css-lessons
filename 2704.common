body{
  margin: 0;
  font-family: 'Oswald', sans-serif;
}

a{
  text-decoration: none;
}
.bg-transparent{
  background-color: rgba(0, 0, 0, 0.75 );
}

nav div a{
  color: white;
  font-size: 18px;
}

small{
  color: yellow;

}

nav .active{
  color:yellow;
}

nav div:first-child a{
  font-size: 40px;

}

nav div:last-child a:hover{
  color: yellow;
}
